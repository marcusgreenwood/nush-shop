<div class="header-container">
    <div class="container">
        <div class="row">
            <div class="column span<%- size[0] || 12 %> module-list">
                <%- column[0] %>
            </div>
        </div>
    </div>
</div>
<div class="container">
  <div class="row">
      <div class="column span<%- size[1] || 8 %>">
          <div class="module-list">
              <%- column[1] %>
          </div>
          <div class="row">
              <div class="column subcolumn span<%- size[2] || 4 %> module-list"><%- column[2] %></div>
              <div class="column subcolumn span<%- size[3] || 4 %> module-list"><%- column[3] %></div>
          </div>
      </div>
      <div class="column span<%- size[4] || 4 %> module-list"><%- column.slice(4).join(' ') %></div>
  </div>
</div>

<div class="cols23">
  <div class="halfHeight">
    <div class="col">1</div>
  </div>
  <div class="halfHeight cols3">
    <div class="col">2</div>
    <div class="col">3</div>
  </div>
</div>
<div class="cols13 cols3">
  <div class="col">4</div>
</div>
